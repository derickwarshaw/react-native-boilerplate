{
  "name": "rnb",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://ethanneff.github.io/react-native-boilerplate",
  "devDependencies": {
    "@commitlint/cli": "^4.2.2",
    "@commitlint/config-angular": "^4.2.1",
    "jest-expo": "^22.0.0",
    "react-native-scripts": "1.6.0",
    "react-test-renderer": "16.0.0"
  },
  "main": "./node_modules/react-native-scripts/build/bin/crna-entry.js",
  "scripts": {
    "start": "react-native-scripts start",
    "app:android": "react-native-scripts android",
    "app:ios": "react-native-scripts ios",
    "app:test": "node node_modules/jest/bin/jest.js --watch",
    "web": "react-scripts start",
    "web:build": "react-scripts build",
    "web:test": "react-scripts test --env=jsdom",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "precommit": "lint-staged",
    "predeploy": "npm run web:build",
    "deploy": "gh-pages -d build",
    "test": "node node_modules/jest/bin/jest.js"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": ["prettier --write", "git add"]
  },
  "commitlint": { "extends": ["@commitlint/config-angular"] },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "expo": "^22.0.0",
    "gh-pages": "^1.0.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.3.0",
    "prettier": "^1.7.4",
    "react": "16.0.0",
    "react-dom": "^16.0.0",
    "react-native": "^0.49.5",
    "react-native-web": "^0.1.13",
    "react-scripts": "^1.0.16"
  }
}
